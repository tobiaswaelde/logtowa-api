{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Backend",
      "program": "${workspaceFolder}/node_modules/@nestjs/cli/bin/nest",
      "args": ["start", "--watch"],
      "runtimeArgs": ["--nolazy"],
      "sourceMaps": true,
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "outputCapture": "std"
    }
  ]
}
